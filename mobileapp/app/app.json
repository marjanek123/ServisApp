{
    "expo": {
        "name": "Servis",
        "slug": "servis",
        "version": "1.3.4",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "assets/*"
        ],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "versionCode": 1,
            "adaptiveIcon": {
                "foregroundImage": "./assets/icon.png",
                "backgroundColor": "#FFFFFF"
            },
            "package": "com.marjanek.servis"
        },
        "web": {
            "favicon": "./assets/icon.png"
        }

    }
}